    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^0.1.16",
    "codecov": "^3.3.0",
    "css-loader": "^2.1.0",
    "doctoc": "^1.3.0",
    "eslint": "^5.16.0",
