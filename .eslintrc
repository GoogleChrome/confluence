{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["google"],
  "rules": {
    "arrow-parens": "off",
    "block-spacing": "off",
    "brace-style": "off",
    "camelcase": "off",
    "comma-dangle": "off",
    "comma-spacing": "off",
    "curly": "off",
    "indent": "off",
    "key-spacing": "off",
    "keyword-spacing": "off",
    "no-invalid-this": "off",
    "no-multi-spaces": "off",
    "no-tabs": "off",
    "no-throw-literal": "off",
    "no-trailing-spaces": "off",
    "no-unused-vars": "off",
    "no-var": "off",
    "new-cap": ["error", {
    	"capIsNew": false
    }],
    "max-len": "off",
    "padded-blocks": "off",
    "prefer-const": "off",
    "prefer-promise-reject-errors": "off",
    "prefer-spread": "off",
    "prefer-rest-params": "off",
    "quotes": "off",
    "require-jsdoc": "off",
    "semi": "off",
    "space-before-function-paren": "off"
  },
  "globals": {
    "foam": false
  },
  "parserOptions": {
    "ecmaVersion": 6
  }
}
